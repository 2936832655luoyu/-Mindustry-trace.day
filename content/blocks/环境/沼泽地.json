{
        "name": "沼泽地",  // 地板名称
        "type": "FloorBlock",  // 方块类型：地板（而非矿石）
        "mapColor": "8B4513FF",  // 地图显示颜色：棕褐色（符合沼泽泥泞质感）
        "useColor": true,  // 启用自定义地图颜色
        "speedMultiplier": 0.7,  // 移动速度倍率：在沼泽上移动速度为70%（模拟泥泞阻力）
        "liquid": "water",  // 关联液体：水（模拟沼泽积水）
        "liquidPadding": 0.15,  // 液体显示高度：30%（部分积水效果）
        "depth": 1,  // 视觉深度：1（在普通地板之上）
        "variant": 3,  // 纹理变体：3（使用第3种沼泽纹理）
        "description": "泥泞的沼泽地带，行走时会受到阻力，表面覆盖着浅层积水"  // 地板描述
    }
