{
    // 矿石名称（显示在游戏内的名称）
    "name": "泥煤地",
    
    // 方块类型（OreBlock 表示这是一个矿石方块）
    "type": "OreBlock",
    
    // 是否允许玩家直接挖掘（true = 可直接挖掘，不需要钻机或特殊工具）
    "playerUnmineable": true,
    
    // 掉落物品（挖掘后获得的物品，这里是"暗蓝石"）
    "itemDrop": "泥煤",
    
    // 地图显示颜色（16进制ARGB颜色，这里使用深蓝色调）
    "mapColor": "8B4513FF",
    
    // 是否使用自定义颜色（true = 使用 mapColor 定义的颜色）
    "useColor": false,
    
    // 是否为默认矿石（false = 不覆盖原版矿石生成）
    "oreDefault": true
}