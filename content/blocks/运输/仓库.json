{
    "name": "仓库",  // 仓库在游戏中显示的名称
    "type": "StorageBlock",  // 建筑类型：存储块，继承基础存储功能
    "description": "大量存储，可卸载器卸载，看小说的时候请把脑子存在这里-------感染",  // 功能说明：支持多类型物品存储，可配合装卸器使用，容量40000
    "itemCapacity": 40000,  // 最大物品存储容量，总计可存放40000个物品（不同类型物品共享此容量）
    "researchCostMultiplier": 0.1
    "hasItems": true,  // 标识该建筑可存储物品
    "health": 3300,  // 仓库的生命值，较高的数值表示更耐攻击
    "size": 5,  // 仓库占地面积为5x5格（1格=32x32像素）
    "requirements": [  // 建造所需材料清单
        "遗湿矿/360",  // 需要360个蓝钢（简写格式：物品名/数量）
        {
            "item": "silicon",
            "amount": 280  // 需要280个硅（标准格式）
        },
        {
            "item": "痕迹合金",
            "amount": 220  // 需要220个钛
        },
        {
            "item": "伸金",
            "amount": 180  // 需要180个石墨
        },
        {
            "item": "压缩铜",
            "amount": 500  // 需要80个冲击合金
        }
    ],
    "category": "effect",  // 建筑分类：效果类，在对应设施栏中显示
    "research": "燥热"  // 解锁该仓库所需的前置研究：通用物品输管
}