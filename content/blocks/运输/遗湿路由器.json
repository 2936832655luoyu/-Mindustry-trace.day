{
    "type": "Router",  // 类型标识，表明这是一个"路由器"类型的物品/建筑
    "name": "遗湿路由器",  // 物品名称，显示给玩家的直观名称
    "speedBoost": 100,  // 速度增益属性，可能是对传输速度的额外加成值
    "description": "路由器，终于可以分流了",  // 物品描述，可能是幽默化的说明文字
    "speed": 100,  // 基础速度属性，可能代表数据/物品的传输速率（注意原配置此处缺少逗号）
    "health": 250,  // 生命值/耐久度，代表该建筑可以承受的伤害值（注意原配置此处缺少逗号）
    "idleSound": "none",  // 闲置状态音效设置，"none"表示无闲置音效（注意原配置此处缺少逗号）
    "requirements": [  // 制作/建造所需材料列表
        {"item": "遗湿矿", "amount": 5},  // 需要5个铜材料（注意原配置数组后多了一个逗号，可能引起格式问题）
    ],
    "category": "distribution",  // 分类属性，属于"分配/分发"类别（注意原配置此处缺少逗号）
    "research": "遗湿传送带",  // 研究前置条件，需要先完成标识为"d"的研究才能解锁（注意原配置此处缺少逗号）
    "placeableLiquid": true  // 液体环境放置许可，true表示可以放置在液体上
}