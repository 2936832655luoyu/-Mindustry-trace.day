{
    "type": "GenericCrafter",  // 建筑类型：通用制造机，继承基础制造建筑的逻辑（如物品输入输出、制造流程）
    "name": "低热能亮室",  // 建筑在游戏内显示的名称，用于界面展示、玩家识别
    "researchCostMultiplier": 0.2
    "category": "crafting",  // 建筑所属分类，决定在建造菜单的归类（制造类建筑会出现在制造相关分页）
    "description": "？？？？—1,-1,-1,然后+5",  // 建筑功能描述，可自定义游戏内展示的说明文本（这里是特殊参数描述）
    "requirements": [  // 建造该建筑所需的材料，玩家需收集对应物品才能解锁建造
        "copper/50",  // 需 50 单位铜（"物品ID/数量" 格式，copper 是原版铜的ID）
        "lead/10"     // 需 50 单位铅（lead 是原版铅的ID）
    ],
    "outputItem": "压缩铅/2",  // 制造完成后产出的物品，格式 "物品ID/数量"。compressed-lead 是原版压缩铅的ID，每次制造产出 4 单位
    "craftTime": 120.00,  // 单次制造流程的耗时（单位：游戏内时间，通常可理解为秒级，数值越大制造越慢）
    "size": 2,  // 建筑占地面积，单位为“格”，2 表示 2x2 的正方形区域
    "hasPower": false,  // 是否需要电力运行，true 表示该建筑为耗电建筑，需接入电力网络
    "hasLiquids": false,  // 是否涉及液体（输入/输出），false 表示仅处理物品和电力
    "drawer": {  // 建筑外观渲染配置，决定在游戏内如何绘制该建筑
        "type": "DrawDefault",  // 官方支持的火焰渲染类型
  
    },
    "consumes": {  // 制造过程中的资源消耗配置，定义运行时需要的输入
        "items": [  // 物品消耗列表，每次制造会扣除对应物品  // 每次制造消耗 4 单位铜
            "lead/4"     // 每次制造消耗 4 单位铅（核心修改点：改为消耗原版铅）
        ],
    },
    "alwaysUnlocked": false,  // 是否默认解锁，false 表示需要通过研究解锁（需配合 research 配置）
   "research": "燥热"
}