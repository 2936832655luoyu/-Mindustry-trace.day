{
    "type": "GenericCrafter",  // 建筑类型：通用制造机，继承基础制造建筑的逻辑（如物品输入输出、制造流程）
    "name": "低能提前器",  // 建筑在游戏内显示的名称，用于界面展示、玩家识别
    "researchCostMultiplier": 0.2
    "category": "crafting",  // 建筑所属分类，决定在建造菜单的归类（制造类建筑会出现在制造相关分页）
    "description": "去去去，不要来烦我，我只是提纯而已",  // 建筑功能描述，可自定义游戏内展示的说明文本（这里是特殊参数描述）
    "requirements": [  // 建造该建筑所需的材料，玩家需收集对应物品才能解锁建造
        "压缩铅/50",  // 需 50 单位铜（"物品ID/数量" 格式，copper 是原版铜的ID）
        "copper/50",
        "压缩铜/100",
        "silicon/70"     // 需 50 单位铅（lead 是原版铅的ID）
    ],
    "outputItem": "伸金/2",  // 制造完成后产出的物品，格式 "物品ID/数量"。compressed-lead 是原版压缩铅的ID，每次制造产出 4 单位
    "craftTime": 120.00,  // 单次制造流程的耗时（单位：游戏内时间，通常可理解为秒级，数值越大制造越慢）
    "size": 2,  // 建筑占地面积，单位为“格”，2 表示 2x2 的正方形区域
    "hasPower": true,  // 是否需要电力运行，true 表示该建筑为耗电建筑，需接入电力网络
    "hasLiquids": false,  // 是否涉及液体（输入/输出），false 表示仅处理物品和电力
    "drawer": {  // 建筑外观渲染配置，决定在游戏内如何绘制该建筑
        "type": "DrawDefault",  // 渲染类型：采用熔炉类建筑的默认渲染逻辑（带火焰动画等样式）

    },
    "consumes": {  // 制造过程中的资源消耗配置，定义运行时需要的输入
        "items": [  // 物品消耗列表，每次制造会扣除对应物品  // 每次制造消耗 4 单位铜
            "遗湿矿/6"     // 每次制造消耗 4 单位铅（核心修改点：改为消耗原版铅）
        ],
        "power": 4  // 电力消耗速率，单位通常为“电力/秒”，持续运行时每秒消耗 0.5 电力
    },
    "alwaysUnlocked": false,  // 是否默认解锁，false 表示需要通过研究解锁（需配合 research 配置）
    "research": {  // 解锁该建筑所需的研究配置
        "parent": "低热能亮室",  // 前置研究 ID（需替换为游戏内实际存在的研究，如原版“燥热科技”是 heat-tech ）
        "requirements": [  // 完成该研究需要消耗的资源
            "lead/210"  // 消耗 100 单位铜来解锁研究
        ]
    }
}