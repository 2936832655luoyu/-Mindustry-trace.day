{
    "type":"PowerNode",  // 类型标识，表明这是一个"电力节点"类建筑
    "name":"脆弱电力传输塔",  // 建筑名称，"(弊)"可能表示该建筑存在某种负面效果或限制
    "description": "很氢，小心轻放啊————常青",  // 功能描述，说明该配电器可覆盖全地图的电力分配
    "size":2,
    "researchCostMultiplier": 0.2// 建筑尺寸，可能代表占地面积或碰撞体积大小
    "health":10,  // 建筑生命值，数值较高说明耐久度很强
    "maxNodes":10,  // 最大连接节点数，888表示可连接大量其他电力节点
    "laserRange":100,  // 激光传输范围，可能是指电力通过激光传输的有效距离
      // 解锁所需的研究标识，需要先完成"f"对应的研究
    "hasPower":true,  // 是否具备电力属性，true表示这是电力相关设备
    "consumesPower": false,  // 是否消耗电力，true表示该建筑自身需要耗电
    "outputsPower": true,  // 是否输出电力，true表示可向其他设备提供电力
    "consumes":{  // 消耗属性配置
        "powerBuffered": 100  // 缓冲电力容量，可储存1000000单位的电力
        // 注意：此处缺少逗号，可能导致解析错误
    },
    "requirements": [  // 建造所需材料列表
        {"item":"遗湿矿","amount":5},  // 需要5个铜材料
        // 注意：数组末尾多了一个逗号，属于格式问题
    ],
    // 解锁条件：必须先占领"降落地"才能解锁当前区域
    "research": {  // 该区域的解锁研究配置（通过完成前置目标解锁）
        "parent": "燥热",  // 研究树中的父节点（通常指前置区域的ID，此处为"groundZero"）
        "objectives": [  // 解锁该区域需要完成的目标列表
            {
                "type": "SectorComplete",  // 目标类型：完成指定区块（占领指定扇区）
                "preset": "降落点"  // 具体目标：需要先完成（占领）"groundZero"区块
            }
        ]
    }
    "category":"power"  // 分类属性，属于"电力"类别
}